{
  "name": "math-functions",
  "version": "0.0.1",
  "description": "A Math Function Evaluator. Piecewise compatible.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "prebuild": "npm run lint:fix && npm run clean && npm rebuild && npm test",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "docs:clean": "rm -rf docs/*.md",
    "docs": "npm run lint:fix && npm run docs:clean && typedoc",
    "postdocs": "echo ©$(date +'%Y') $(cat package.json | grep \\\"author\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\\\",]//g') > docs/_Footer.md",
    "lint:eslint": "eslint ./src/*.ts",
    "lint:fix": "eslint ./src/*.ts --fix"
  },
  "keywords": [
    "math",
    "func",
    "eval",
    "piecewise"
  ],
  "author": "Daniel Sales Álvarez <danisales.es@gmail.com>",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ByDSA/math-functions.git"
  },
  "bugs": {
    "url": "https://github.com/ByDSA/math-functions/issues"
  },
  "homepage": "https://github.com/ByDSA/math-functions#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.32",
    "typedoc-github-wiki-theme": "^0.2.0",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "^4.2.3"
  }
}